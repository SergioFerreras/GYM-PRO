<AnchorPane minHeight="715.0" minWidth="100.0" prefHeight="161.0" prefWidth="415.0" styleClass="background-color" stylesheets="@../CSS/style.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.gympro.Controller.UserSignUpController">
    <children>
        <Button fx:id="loginButton" layoutX="15.0" layoutY="29.0" mnemonicParsing="false" onAction="#irLogin" styleClass="botonTransparente" stylesheets="@../CSS/ButtonsStyle.css" text="Login" textFill="WHITE">
