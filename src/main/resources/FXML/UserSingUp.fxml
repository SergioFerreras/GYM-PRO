            <Insets />
         </opaqueInsets>
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></DatePicker>
        <Button fx:id="continueButton" layoutX="148.0" layoutY="646.0" mnemonicParsing="false" onAction="#continuar" prefHeight="40.0" prefWidth="120.0" styleClass="buttonColorVerde" stylesheets="@../CSS/ButtonsStyle.css" text="Continue &gt;">
            <font>
                <Font size="16.0" />
            </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
        </Button>
        <ImageView fitHeight="138.0" fitWidth="129.0" layoutX="143.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
