            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="cpasswordTF" layoutX="83.0" layoutY="316.0" prefHeight="21.0" prefWidth="250.0" promptText="Confirm Password" styleClass="text-field" stylesheets="@../CSS/style.css">
