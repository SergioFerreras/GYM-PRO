                <Image url="@../Elements/Icons/Logo.jpg" />
            </image>
        </ImageView>
        <Label layoutX="98.0" layoutY="140.0" prefHeight="30.0" prefWidth="219.0" text="Welcome to GYM-PRO" textFill="#acf564">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
      <TextField fx:id="telphonenTF" layoutX="83.0" layoutY="401.0" prefHeight="26.0" prefWidth="250.0" promptText="Telephone Number" styleClass="text-field" stylesheets="@../CSS/style.css">
