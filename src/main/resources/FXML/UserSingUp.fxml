                <ColorAdjust brightness="-0.04" />
            </effect>
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
        </TextField>
        <TextField fx:id="surnameTF" layoutX="223.0" layoutY="228.0" prefHeight="26.0" prefWidth="111.0" promptText="Surnames" styleClass="text-field" stylesheets="@../CSS/style.css">
