                <Font name="System Bold" size="10.0" />
            </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
        </Button>
        <Button layoutX="49.0" layoutY="27.0" mnemonicParsing="false" styleClass="loginSingupSelected" stylesheets="@../CSS/ButtonsStyle.css" text="Sign up" textFill="WHITE">
            <font>
                <Font name="System Bold" size="10.0" />
            </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
        </Button>
        <TextField fx:id="nameTF" layoutX="85.0" layoutY="228.0" prefHeight="26.0" prefWidth="111.0" promptText="Name" stylesheets="@../CSS/style.css">
