      <Label layoutX="171.0" layoutY="363.0" text="Email/Telephone:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="9.0" />
         </font>
      </Label>
      <Label layoutX="188.0" layoutY="277.0" text="Pasword:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="9.0" />
         </font>
      </Label>
      <Label layoutX="192.0" layoutY="450.0" text="Adress:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="9.0" />
         </font>
      </Label>
      <Label layoutX="155.0" layoutY="511.0" text="Identification document:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="9.0" />
         </font>
      </Label>
      <Label layoutX="166.0" layoutY="570.0" text="Birthday  (1/1/2000):" textFill="WHITE">
         <font>
            <Font name="System Bold" size="9.0" />
         </font>
      </Label>
      <Button fx:id="crossButton" layoutX="385.0" layoutY="15.0" mnemonicParsing="false" onAction="#cerrarVentana" prefHeight="0.0" prefWidth="16.0" styleClass="crossButton" stylesheets="@../CSS/ButtonsStyle.css">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
    </children>
</AnchorPane>
