            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="adressTF" layoutX="90.0" layoutY="463.0" prefHeight="26.0" prefWidth="162.0" promptText="Adress" styleClass="text-field" stylesheets="@../CSS/style.css">
